      "diceParsing": [
        {
          "regex": "\\((\\d+d\\d+(?:\\s*[+-]\\s*\\d+)?)\\)",
          "parser": "const [, text] = matches;\nreturn [\"(\", { text }, \")\"];",
          "id": "5bba0b0b988b",
          "desc": "(1d8+3)"
        },
        {
          "regex": "(\\w+?) ([+-])(\\d+)",
          "parser": "let [, initial, sign, number] = matches;\nlet mult = 1;\nif (sign === \"-\") {\n    mult = -1;\n}\nlet text;\nif (!isNaN(Number(number))) {\n    text = `1d20+${mult * Number(number)}`;\n}\nreturn [original, \" (\", { text }, \")\"];",
